<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="660.0" prefWidth="990.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="App.MissionControl.MissionControl_Controller">
   <children>
      <ImageView fx:id="image" fitHeight="660.0" fitWidth="990.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@DailyMission/DailyMission.png" />
         </image>
      </ImageView>
      <BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="660.0" prefWidth="990.0">
         <left>
            <VBox prefHeight="624.0" prefWidth="200.0" spacing="15.0" BorderPane.alignment="CENTER">
               <children>
                  <Button mnemonicParsing="false" onAction="#showDailyMission" prefHeight="26.0" prefWidth="180.0" text="Daily Mission" />
                  <Button mnemonicParsing="false" onAction="#showProgress" prefHeight="26.0" prefWidth="180.0" text="Progress" />
                  <Button mnemonicParsing="false" onAction="#showProject" prefHeight="26.0" prefWidth="180.0" text="Project" />
               </children>
            </VBox>
         </left>
         <center>
            <StackPane fx:id="contentPane" prefHeight="624.0" prefWidth="790.0" BorderPane.alignment="CENTER" />
         </center>
         <top>
            <AnchorPane prefHeight="36.0" prefWidth="990.0" BorderPane.alignment="CENTER">
               <children>
                  <Button mnemonicParsing="false" onAction="#exit" prefHeight="26.0" prefWidth="41.0" text="Exit" />
               </children>
            </AnchorPane>
         </top>
      </BorderPane>
   </children>
</AnchorPane>
